{
  "name": "action-expose",
  "description": "GitHub Action para exportar as secrets no formato 'KEY_1=value1 KEY_2=value2 KEY_3=value3'.",
  "version": "1.0.0",
  "private": true,
  "homepage": "https://github.com/weenow/action-expose",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/weenow/action-expose.git"
  },
  "bugs": {
    "url": "https://github.com/weenow/action-expose/issues"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "exports": {
    ".": "./dist/index.js"
  },
  "engines": {
    "node": "18.18.2"
  },
  "scripts": {
    "bundle": "npm run format:write && npm run package",
    "format:write": "npx prettier --write .",
    "format:check": "npx prettier --check .",
    "lint": "npx eslint . -c ./.github/linters/.eslintrc.yml",
    "package": "npx ncc build src/index.ts -o dist --source-map --license licenses.txt",
    "package:watch": "npm run package -- --watch",
    "all": "npm run format:write && npm run lint && npm run package"
  },
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.10.1"
  },
  "devDependencies": {
    "@types/node": "^20.14.8",
    "@typescript-eslint/eslint-plugin": "^7.13.1",
    "@typescript-eslint/parser": "^7.13.1",
    "@vercel/ncc": "^0.38.2",
    "eslint": "^8.57.0",
    "eslint-plugin-github": "^5.0.2",
    "eslint-plugin-jsonc": "^2.16.0",
    "eslint-plugin-prettier": "^5.2.1",
    "prettier": "^3.3.3",
    "prettier-eslint": "^16.3.0",
    "typescript": "^5.6.2"
  }
}
